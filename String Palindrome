class Main
{
    public static void main(String[] args)
    {
        String s="siva";
        String os=s;
        String ans="";
        for(int i=s.length()-1;i>=0;i--)
        {
            ans+=s.charAt(i);
        }
        if(ans.equals(s))
        {
            System.out.println("Palindrome");
        }
        else
        {
            System.out.println("It is not a Palindrome");
        }
        System.out.print(ans);
    }
}
